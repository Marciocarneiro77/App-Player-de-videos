<?xml version='1.0' encoding='utf-8'?>
<widget id="com.marcioCarneiro.appPlayer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Player de videos</name>
    <description>Aplicativo de videos</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>

<content src="index.html" />
    <icon src="res/video.png" />

<preference name= "SplashScreen" value="screen" />
    <splash src="res/video.png" />

    

    <preference name= "Fullscren" value="true" />

    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
</widget>
